  <!--== Laser Weapons ==-->
  <ThingDef ParentName="BaseHumanGun">
    <defName>Gun_LaserPistol</defName>
    <label>Laser Pistol</label>
    <description>Highly advanced pattern laser handgun. Lightweight, accurate and easy to use. Most effective at shorter ranges.</description>
    <graphicData>
      <texPath>Things/Weapons/Tier5A/Pistol</texPath>
      <graphicClass>Graphic_Flicker</graphicClass>
    </graphicData>
    <soundInteract>Interact_Autopistol</soundInteract>
    <statBases>
      <WorkToMake>25000</WorkToMake>
      <Mass>1.25</Mass>
      <AccuracyTouch>0.95</AccuracyTouch>
      <AccuracyShort>0.85</AccuracyShort>
      <AccuracyMedium>0.75</AccuracyMedium>
      <AccuracyLong>0.65</AccuracyLong>
      <RangedWeapon_Cooldown>1.25</RangedWeapon_Cooldown>
    </statBases>
    <costList>
      <Steel>25</Steel>
      <ComponentIndustrial>4</ComponentIndustrial>
      <Plasteel>25</Plasteel>
    </costList>
    <verbs>
      <li>
        <verbClass>Verb_Shoot</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <defaultProjectile>Bullet_Laser</defaultProjectile>
        <warmupTime>0.75</warmupTime>
        <range>22</range>
        <burstShotCount>3</burstShotCount>
        <ticksBetweenBurstShots>40</ticksBetweenBurstShots>
        <soundCast>ShotLaser</soundCast>
        <soundCastTail>GunTail_Light</soundCastTail>
        <muzzleFlashScale>9</muzzleFlashScale>
      </li>
    </verbs>
    <equippedStatOffsets>
      <MoveSpeed>0</MoveSpeed>
    </equippedStatOffsets>
    <recipeMaker>
      <workSpeedStat>SmithingSpeed</workSpeedStat>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Smith</soundWorking>
      <unfinishedThingDef>UnfinishedGun</unfinishedThingDef>
      <researchPrerequisite>LaserResearch</researchPrerequisite>
      <recipeUsers>
        <li>AdvancedTableMachining</li>
      </recipeUsers>
      <workSkill>Crafting</workSkill>
    </recipeMaker>
    <comps>
      <li Class="CompProperties_Glower">
        <glowRadius>1</glowRadius>
        <glowColor>(255,55,55,0)</glowColor>
      </li>
      <li Class="CompProperties_HeatPusher">
        <compClass>CompHeatPusherPowered</compClass>
        <heatPerSecond>1</heatPerSecond>
      </li>
    </comps>
    <tools>
      <li>
        <label>grip</label>
        <capacities>
          <li>Blunt</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.6</cooldownTime>
      </li>
      <li>
        <label>barrel</label>
        <capacities>
          <li>Blunt</li>
          <li>Poke</li>
        </capacities>
        <power>8</power>
        <cooldownTime>1.6</cooldownTime>
      </li>
    </tools>
    <weaponTags>
      <li>EliteGun</li>
    </weaponTags>
  </ThingDef>
